{
  "plugins": ["plugins/markdown", "jsdoc-http-plugin"],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "exclude": [
      ".vscode",
      "dist",
      "coverage",
      "cypress",
      "docs",
      "node_module",
      "public",
      "tutorials"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "title": "Template Next",
      "meta": [
        {
          "name": "author",
          "content": "nico1311"
        },
        {
          "name": "description",
          "content": "Example project"
        }
      ],
      "footer": "<footer>Â© Copyright 2021. All rights reserved.</footer>"
    }
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
